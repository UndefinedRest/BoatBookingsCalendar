{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Booking Viewer",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/tsx/dist/cli.mjs",
      "args": ["src/index.ts"],
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Booking Viewer (Debug Mode)",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/tsx/dist/cli.mjs",
      "args": ["src/index.ts"],
      "console": "integratedTerminal",
      "env": {
        "REVSPORT_DEBUG": "true"
      },
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}